Program ::= <类型> 空白 <ID> '(' ')' <语句块>
<类型> ::= 'int' | 'void'
<语句块> ::= '{' <内部声明> <语句串> '}'
<内部声明> ::= <内部变量声明>*
<内部变量声明> ::= 'int' 空白 <ID> ';'
<语句串> ::= <语句>*
<语句> ::= <if语句> | <while语句> | <return语句> | <赋值语句>
<赋值语句> ::= <ID> '=' <表达式> ';'
<return语句> ::= <return> 空白? <表达式>? ';'
<return> ::= 'return'
<while语句> ::= 'while' '(' <表达式> ')' <语句块>
<if语句> ::= 'if' '(' <表达式> ')' <语句块> ('else' <语句块>)?
<表达式> ::= <加法表达式> (RELOP <加法表达式>)*
<加法表达式> ::= <项> ('+' <项> | '-' <项>)*
<项> ::= <因子> ('*' <因子> | '/' <因子>)*
<因子> ::= 数值 | '(' <表达式> ')' | <ID> FTYPE
FTYPE ::= <call>?
<call> ::= '(' <实参列表> ')'
<实参> ::= <实参列表>?
<实参列表> ::= <表达式> (',' <表达式>)*
RELOP ::= '<' | '<=' | '>' | '>=' | '==' | '!='
字母 ::= [a-zA-Z]
<ID> ::= 字母 (字母 | 数字)*
数字 ::= [0-9]
数值 ::= [0-9]+
空白 ::= [#x20#x9#xA#xD]+
